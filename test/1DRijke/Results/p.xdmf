<?xml version="1.0"?>
<!DOCTYPE Xdmf SYSTEM "Xdmf.dtd" []>
<Xdmf Version="3.0" xmlns:xi="http://www.w3.org/2001/XInclude">
  <Domain>
    <Grid Name="mesh" GridType="Uniform">
      <Topology TopologyType="PolyLine" NumberOfElements="40" NodesPerElement="2">
        <DataItem Dimensions="40 2" NumberType="Int" Format="HDF">p.h5:/Mesh/mesh/topology</DataItem>
      </Topology>
      <Geometry GeometryType="XY">
        <DataItem Dimensions="41 2" Format="HDF">p.h5:/Mesh/mesh/geometry</DataItem>
      </Geometry>
    </Grid>
    <Grid Name="P" GridType="Collection" CollectionType="Temporal">
      <Grid Name="P" GridType="Uniform">
        <xi:include xpointer="xpointer(/Xdmf/Domain/Grid[@GridType='Uniform'][1]/*[self::Topology or self::Geometry])" />
        <Time Value="0" />
        <Attribute Name="real_P" AttributeType="Scalar" Center="Node">
          <DataItem Dimensions="41 1" Format="HDF">p.h5:/Function/real_P/0</DataItem>
        </Attribute>
        <Attribute Name="imag_P" AttributeType="Scalar" Center="Node">
          <DataItem Dimensions="41 1" Format="HDF">p.h5:/Function/imag_P/0</DataItem>
        </Attribute>
      </Grid>
    </Grid>
  </Domain>
</Xdmf>
